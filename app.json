{
  "expo": {
    "name": "Cafe Khanevadeh",
    "slug": "CafeKhanevadeh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cafe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    // "splash": {
    //   "image": "./assets/images/splash.png",
    //   "resizeMode": "contain",
    //   "backgroundColor": "#161514"
    // },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "ExpoLocalization_forcesRTL": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#161514"
      },
      "package": "com.pooyam99.cafekhanevadeh"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#161514",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Vazirmatn-FD-Black.ttf",
            "./assets/fonts/Vazirmatn-FD-Bold.ttf",
            "./assets/fonts/Vazirmatn-FD-ExtraBold.ttf",
            "./assets/fonts/Vazirmatn-FD-ExtraLight.ttf",
            "./assets/fonts/Vazirmatn-FD-Light.ttf",
            "./assets/fonts/Vazirmatn-FD-Medium.ttf",
            "./assets/fonts/Vazirmatn-FD-Regular.ttf",
            "./assets/fonts/Vazirmatn-FD-SemiBold.ttf",
            "./assets/fonts/Vazirmatn-FD-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true,
      "forcesRTL": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b82960ad-bcfb-4e39-818f-209fbbdef5ad"
      }
    },
    "owner": "pooyam99"
  }
}
