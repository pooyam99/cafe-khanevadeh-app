{
  "expo": {
    "name": "Cafe Khanevadeh",
    "slug": "Cafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cafe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#161514"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Vazirmatn-FD-Black.ttf",
            "./assets/fonts/Vazirmatn-FD-Bold.ttf",
            "./assets/fonts/Vazirmatn-FD-ExtraBold.ttf",
            "./assets/fonts/Vazirmatn-FD-ExtraLight.ttf",
            "./assets/fonts/Vazirmatn-FD-Light.ttf",
            "./assets/fonts/Vazirmatn-FD-Medium.ttf",
            "./assets/fonts/Vazirmatn-FD-Regular.ttf",
            "./assets/fonts/Vazirmatn-FD-SemiBold.ttf",
            "./assets/fonts/Vazirmatn-FD-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true
    }
  }
}
